<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/sectionschooselevelframelayout"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="@drawable/bg1"
        android:padding="0dip">
        <LinearLayout
		    android:id="@+id/sectionschooselayout"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:orientation="vertical"
		    android:gravity="center_vertical"
		    >
		    <!--  
		    	<Gallery
		    	android:id="@+id/gallerysections"
			        android:layout_width="fill_parent"
			        android:layout_height="wrap_content"/> 
			        -->
		</LinearLayout>
		<LinearLayout
				    android:layout_width="fill_parent"
				    android:layout_height="fill_parent"
				    android:orientation="horizontal"
				    android:gravity="center_horizontal"
			    >
			     <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
						    android:layout_width="wrap_content"
						    android:layout_height="fill_parent"
						    android:orientation="vertical"
						    android:gravity="bottom"
						    >
						    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
								    android:layout_width="wrap_content"
								    android:layout_height="wrap_content"
								    android:orientation="horizontal"
								    android:gravity="center_horizontal"
								    >
								    <LinearLayout
									    android:layout_width="wrap_content"
									    android:layout_height="fill_parent"
									    android:orientation="vertical"
									    android:gravity="center_vertical"
									    >
								    	<ImageView android:id="@+id/starimagelabel"
								             android:layout_width="35dip"
								             android:layout_height="35dip"
								             android:src="@drawable/stella_black"
								             />
									</LinearLayout>
									<LinearLayout
									    android:layout_width="wrap_content"
									    android:layout_height="fill_parent"
									    android:orientation="vertical"
									    android:gravity="center_vertical"
									    >
									    <TextView 
									    	android:id="@+id/starimagetext"
									        android:layout_width="wrap_content"
									        android:layout_height="wrap_content"
									        android:text=""/>
									</LinearLayout>
						     </LinearLayout>
			        </LinearLayout>
			    	
	                
		</LinearLayout>
		
		<LinearLayout
		    android:id="@+id/iconcinedisegno"
		    android:layout_width="fill_parent"
		    android:layout_height="fill_parent"
		    android:orientation="vertical"
		    android:gravity="bottom"
		    android:layout_gravity="bottom"
		    >
		     
		    <LinearLayout
		    android:layout_width="fill_parent"
		    android:layout_height="wrap_content"
		    android:orientation="horizontal"
		    >
				    <LinearLayout
				    android:layout_width="wrap_content"
				    android:layout_height="wrap_content"
				    android:orientation="horizontal"
				    >
					    <ImageView android:id="@+id/secimage1"
								               android:layout_width="200dp"
								               android:layout_height="200dp"
								               android:src="@drawable/jhonnybrush_9"
								               />
					</LinearLayout>
					<!--  
					<LinearLayout
					    android:layout_width="fill_parent"
					    android:layout_height="wrap_content"
					    android:orientation="horizontal"
					    android:gravity="right"
					    android:layout_gravity="right"
					    >
		                <ImageView android:id="@+id/secimage2"
								               android:layout_width="200dp"
								               android:layout_height="200dp"
								               android:src="@drawable/rb"
								               />
					</LinearLayout>
					-->
            </LinearLayout>
            
		</LinearLayout>
		
</FrameLayout>